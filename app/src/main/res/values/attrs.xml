<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BtnDropShadow">
        <attr name="dropShadow" format="integer" />
        <attr name="dropWhenIsActive" format="boolean" />
    </declare-styleable>

    <declare-styleable name="LabeledInput">
        <attr name="labelText" format="string" />
        <attr name="inputValue" format="string" />
        <attr name="canModify" format="boolean" />
        <attr name="inputValueColor" format="color" />
        <attr name="inputValueTextModification" format="enum">
            <enum name="select_birthday" value="2" />
            <enum name="underline" value="1" />
            <enum name="none" value="0" />
        </attr>
        <attr name="inputHint" format="string" />
        <attr name="backgroundResource" format="reference" />
        <attr name="labelTextSize" format="dimension" />
    </declare-styleable>
</resources>